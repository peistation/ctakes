<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>NamedEntity</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <imports>
    <import location="IdentifiedAnnotation.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.mayo.bmi.uima.core.type.NamedEntity</name>
      <description/>
      <supertypeName>edu.mayo.bmi.uima.core.type.IdentifiedAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>discoveryTechnique</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>ontologyConceptArr</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>status</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>certainty</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>typeID</name>
          <description>The type of named entity (e.g. drug, disorder, ...)</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence of the annotation.</description>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>segmentID</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sentenceID</name>
          <description>contains the sentence id of the sentence that contains the NE's text span</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>drugMentionArr</name>
          <description>holds drugMention annotations</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
