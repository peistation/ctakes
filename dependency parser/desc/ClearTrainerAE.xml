<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.mayo.bmi.nlp.parser.ae.ClearTrainerAE</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>ClearTrainerAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>DependencyModelFile</name>
        <description>Grammar, lexicon, etc for the Clear Parser's Trainer'</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>LexiconDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>FeatureTemplateFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TrainerPath</name>
        <description>Path to the C++ liblinear trainer.
If this is not a valid file, ClearTrainerAE will use the Java version (less efficient).</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TrainerSolver</name>
        <description>Picks which solver is used in liblinear (default 3):
  0 -- L2-regularized logistic regression (primal)
  1 -- L2-regularized L2-loss support vector classification (dual)
  2 -- L2-regularized L2-loss support vector classification (primal)
  3 -- L2-regularized L1-loss support vector classification (dual)
  4 -- multi-class support vector classification by Crammer and Singer
  5 -- L1-regularized L2-loss support vector classification
  6 -- L1-regularized logistic regression
  7 -- L2-regularized logistic regression (dual)
</description>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TrainerCost</name>
        <description>The parameter C in SVM.</description>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TrainerEpsilon</name>
        <description>The tolerance of termination criterion.</description>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TrainerBias</name>
        <description>If bias &gt;= 0, instance x becomes [x;bias]; if &lt; 0, no bias term added (default -1)</description>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>DependencyModelFile</name>
        <value>
          <string>resources/clinques.mod</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>FeatureTemplateFile</name>
        <value>
          <string>resources/feature.xml</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>LexiconDirectory</name>
        <value>
          <string>lexicon</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TrainerPath</name>
        <value>
          <string>../../liblinear-1.7/train</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TrainerSolver</name>
        <value>
          <integer>3</integer>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TrainerCost</name>
        <value>
          <float>0.1</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TrainerEpsilon</name>
        <value>
          <float>0.1</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TrainerBias</name>
        <value>
          <float>-1.0</float>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="TypeSystem.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities>
      <priorityList>
        <type>edu.mayo.bmi.uima.core.type.Segment</type>
        <type>edu.mayo.bmi.uima.core.type.Sentence</type>
        <type>edu.mayo.bmi.uima.core.type.BaseToken</type>
      </priorityList>
    </typePriorities>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
