<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>BaseDependencyNode</name>
  <description>A basic node for dependency parsing, simplified from the CONLL-X data format but with fewer features.  Coincides with the Malt parser's malttab format.</description>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>edu.mayo.bmi.nlp.parser.type.BaseDependencyNode</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>FORM</name>
          <description>The lexical item itself.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>POSTAG</name>
          <description>Part of speech for the lexical item.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>DEPREL</name>
          <description>Label/type/relation for dependency edges.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
