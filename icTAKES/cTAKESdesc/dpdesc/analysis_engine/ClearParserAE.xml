<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.mayo.bmi.nlp.parser.ae.ClearParserAE</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>ClearParserAE</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>DependencyModelFile</name>
        <description>Grammar, lexicon, etc for the Clear Parser</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>LexiconDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>FeatureTemplateFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>MorphDictionaryDirectory</name>
        <description>Location of the dictionary for Morphy, WordNet's English morphological analyzer.  This provides lemmas for use in ClearParser.

If no valid directory is given, this AE will default to using lemmas provided by BaseToken's normalizedForm field in the CAS.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>DependencyModelFile</name>
        <value>
          <string>dpresources/clinques.mod</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>LexiconDirectory</name>
        <value>
          <string>dpresources/lexiconClinQues</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>FeatureTemplateFile</name>
        <value>
          <string>dpresources/feature.xml</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>MorphDictionaryDirectory</name>
        <value>
          <string>dpresources/en_dict</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import location="../../typesystem/cTAKESTypes.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities>
      <priorityList>
        <type>edu.mayo.bmi.uima.core.type.Segment</type>
        <type>edu.mayo.bmi.uima.core.type.Sentence</type>
        <type>edu.mayo.bmi.uima.core.type.BaseToken</type>
      </priorityList>
    </typePriorities>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type>edu.mayo.bmi.uima.core.type.BaseToken</type>
          <feature>edu.mayo.bmi.uima.core.type.BaseToken:partOfSpeech</feature>
          <feature>edu.mayo.bmi.uima.core.type.BaseToken:normalizedForm</feature>
          <feature>edu.mayo.bmi.uima.core.type.BaseToken:tokenNumber</feature>
          <feature>edu.mayo.bmi.uima.core.type.BaseToken:end</feature>
          <feature>edu.mayo.bmi.uima.core.type.BaseToken:begin</feature>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">edu.mayo.bmi.nlp.parser.type.ConllDependencyNode</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
